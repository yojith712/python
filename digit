num=int(input("Enter a digit"))

if(num>=0 and num<=9):
    print("First digit")

elif(num>=10 and num<=99):
    print("two digits")

elif(num>=100 and num<=999):
    print("three digit")

elif(num>=1000 and num<=9999):
    print("four digit")

elif(num>=10000):
    print("Large digit")

else:
    print("- is not allowed")

